            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" id="myModalLabel">{% trans %}Give us your opinion{% endtrans %}</h4>
                    </div>
                    <form class="form-horizontal" role="form" action="{{ path('send_opinion')}}" method="POST"  enctype="multipart/form-data">
                        <div class="modal-body">
                            <p>{% trans %}modal.opinion.text{% endtrans %}</p>

                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-6">
                                        <h4>{% trans %}About you{% endtrans %}</h4>

                                        <div class="form-group">
                                            <label for="inputName" class="col-sm-4 control-label">{% trans %}Name{% endtrans %}</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="inputName" name="inputName"
                                                       required="">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputEmail" class="col-sm-4 control-label">{% trans %}Email{% endtrans %}</label>
                                            <div class="col-sm-8">
                                                <input type="email" class="form-control" id="inputEmail"
                                                       name="inputEmail" required="">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputDate" class="col-sm-4 control-label">{% trans %}Fecha de su visita{% endtrans %}</label>
                                            <div class="col-sm-8">
                                                <input type="date" class="form-control" id="inputDate" placeholder="12/31/2016" name="inputDate">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputTime" class="col-sm-4 control-label">{% trans %}Hora{% endtrans %}</label>
                                            <div class="col-sm-8">
                                                <input type="time" class="form-control" id="inputTime" placeholder="12:15 AM" name="inputTime">
                                            </div>                                            
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <h4>{% trans %}Message{% endtrans %}</h4>
                                        <div class="form-group">
                                            <textarea maxlength="300" name="inputMessage" required=""></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">{% trans %}Agrege su foto a nuestra Galería{% endtrans %}
                                    <!-- MAX_FILE_SIZE must precede the file input field -->
                                    <input type="hidden" name="MAX_FILE_SIZE" value="3000000" />
                                    <input type="file" name="inputPhoto"> </div>
                            </div>
                            <div class="col-md-12">

                                <hr>
                                <label for="inputTime" class="col-md-12 control-label">{% trans %}No eres un robot, ¿cierto?{% endtrans %}</label>

                                <div class="col-md-6">
                                    <label style="font-weight: 300;">{{ pregunta.getTransQuestion(locale) }}</label>
                                </div>
                                <div class="col-md-6 human-answers">
                                    <input type="hidden" name="inputhidden" value="{{ pregunta.id }}">
                                    {% for value in pregunta.getTransListOptions(locale) %}
                                        <input type="radio" name="answer" value="{{ value }}" id="radio_{{ value }}"
                                               required="">
                                        <label for="radio_{{ value }}">{{ value }}</label>
                                    {% endfor %}
                                </div>
                            </div>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">{% trans %}Close{% endtrans %}</button>
                            <button type="submit" class="btn btn-primary">{% trans %}Send{% endtrans %}</button>
                        </div>
                    </form>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->

