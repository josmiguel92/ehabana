{% extends 'base.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCefOgb1ZWqYtj7raVSmN4PL2WkTrc-KyA"
        xmlns="http://www.w3.org/1999/html"></script>
<script src="{{ asset("js/google_map.js") }}"></script>
{% endblock javascripts %}
{% block body %}
<div id="fh5co-page" class="{% block pageclass %}{% endblock pageclass %}intro-effect-push">
<header id="fh5co-header" role="banner">
    <div class="container">
        <div class="header-inner">
            <a href="{{ path("homepage") }}"><img id="logo" src="{{ asset("images/logo_red.png") }}"></a>
            <nav role="navigation" id="navbar-top" class="navbar navbar-default navbar-static">
                <ul class="nav">
                    <li><a href="#start">{% trans %}Start{% endtrans %}</a></li>
                    <li><a href="#foods">{% trans %}Food{% endtrans %}</a></li>
                    <li><a href="#drinks">{% trans %}Drinks{% endtrans %}</a></li>
                    <li><a href="#about">{% trans %}About Us{% endtrans %}</a></li>
                    <li><a href="#contact">{% trans %}Contact{% endtrans %}</a></li>
                    <li><a href="#galery">{% trans %}galery{% endtrans %}</a></li>
                    <li>
                        <a href="#" class="btn btn-primary btn-outline btn-sm header-btn" id="BookingBtnTop" data-toggle="modal" data-target="#BookingModal">{% trans %}Reservar ahora{% endtrans %}</a>
                    </li>
                    <li>
                        {% if config['social.fb'] is defined %}
                            <a href="{{ config['social.fb'][locale] }}" class="offcanvas-social">
                        <i class="icon-facebook"></i>
                    </a>
                        {% endif %}
                        {#% if config['social.instagram'] is defined %}
                            <a href="{{ config['social.instagram'][locale] }}" class="offcanvas-social">
                        <i class="icon-instagram"></i>
                    </a>
                        {% endif %#}
                        {% if config['social.tripadvisor'] is defined %}
                            <a href="{{ config['social.tripadvisor'][locale] }}" class="offcanvas-social">
                        <i class="icon-tripadvisor"></i>
                    </a>
                        {% endif %}
                    </li>
                    

                </ul>
            </nav>
        </div>
    </div>
</header>
    {% block header %}
    <header class="header js-fullheight">
        <div class="bg-img"><img src="{{ asset("images/table.jpg") }}" alt="Elizalde Habana"/></div>
        <div id="locale-selector" class="animate-box"><a href="{{ path('homepage', {"_locale":"es"}) }}"><i
                        class="hidden">ESP</i><img src="{{ asset("images/spain.png") }}" height="30em" width="45em">
            </a> | <a href="{{ path("homepage", {"_locale":"en"}) }}"><i class="hidden">ENG</i><img
                        src="{{ asset("images/uk.png") }}" height="30em" width="45em"></a></div>
        <div class="title animate-box image" style="left: 0px;">

            {# cambiar logo en casos especificos segun fecha #}
            {% if 'now' | date('z') > 340 or 'now' | date('z') < 6 %}
            <img src="{{ asset("images/signature_alternative_hat.png") }}" class="signature">
            {% else %}
            <img src="{{ asset("images/signature_alternative.png") }}" class="signature">
            {% endif %}
            {#

            <h1>


            {% if config['header.title'] is defined %}

            {{ utils.breakLinesByBars( config['header.title'][locale] ) | nl2br }}
            {% else %}
            {% trans %}Bienvenidos al Restaurante-Bar Elizalde{% endtrans %}
            {% endif %}</h1>
            #}
        </div>
    </header>
    <button class="trigger" data-info="Click to see the content"><span>Trigger</span></button>


    {% endblock header %}

    <div class="container" id="start">
        {% block msg %}{% endblock msg %}
    </div>



<article class="content">
    {% block hero %}
    {% if carteles is not null %}
    <div id="fh5co-hero" class="content js-fullheight">
        <div class="flexslider">
            <ul class="slides">
            {% for cartel in carteles %}
                <li class="js-fullheight" data-count="{{ loop.index }}">
                    <div class="overlay-gradient"></div>
                    <div class="container">
                        <div class="row">
                            <div class="col-md-5 image-container full-image"
                                 style="background: url({{ asset(cartel.getWebPath()) }})  50% 50% /100% no-repeat;height: calc(70vh - 5px);">

                            </div>
                            <div class="col-md-7" style="/*! margin-right: 20px; */padding-left: 3em;">
                                <h2>{{ cartel.getTransTitle(locale) }}</h2>

                                {#
                                  {% if cartel.getMoreInfo(locale) %}
                                    <a href="#" class="btn btn-primary btn-sm more-info-btn">+ Info</a>

                                    <div class="col-md-12 more-info">
                                    <div class="col-md-5 image-container">
                                        <img src="{{ asset(cartel.getWebPath()) }}" >
                                    </div>
                                        <p>{{cartel.getTransAllResumen(locale) | nl2br}}</p>
                                    </div>
                                    {% endif %}
                                  <p>{{cartel.getTransResumen(locale)}}</p> #}
                                <p>{{ cartel.getTransAllResumen(locale) | nl2br }}</p>
                            </div>
                        </div>

                    </div>
                </li>
            {% endfor %}
            </ul>


        </div>

    </div>
    {% endif %}
    {% endblock hero %}
